    jQuery(document).ready(function($) {
      // Quick solution to update absolute links to relative paths until this
      // can be done at database level.
      // Applies only to the conditional sites listed below.
      var siteURL = 'https://' + top.location.host.toString();
      if (siteURL == 'https://bhrrc.org') {
        // Find all links starting with https://business-humanrights.org/.
        var $internalLinks = $("a[href^='https://business-humanrights.org/']");
        $.each( $internalLinks, function( key, element ) {
          // Replace the link with a relative path.
          var href = $(element).attr('href');
          href = href.replace('https://business-humanrights.org/', '/');
          $(element).attr('href', href);
        });

        // Find all form actions starting with https://business-humanrights.org/.
        var $forms = $("form[action^='https://business-humanrights.org/']");
        $.each( $forms, function( key, form ) {
          // Replace the link with a relative path.
          var action = $(form).attr('action');
          action = action.replace('https://business-humanrights.org/', '/');
          $(form).attr('action', href);
        });
      }
    });
  </script>
